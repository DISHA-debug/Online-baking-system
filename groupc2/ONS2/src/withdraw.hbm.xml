<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


  <hibernate-mapping>
  <class name="ONS.withdraw" table="deposittable">
  <id name="idcard">
  <generator class="assigned"></generator></id>
  <property name="withdrawamount" column="withdrawamount" type="int"></property>
  <property name="totalamount" column="totalamount" type="int"></property>
  <property name="fname" column="fname" type="string"></property>
  <property name="lname" column="lname" type="string"></property>
  <property name="acctype" column="acctype" type="string"></property>
  <property name="password" column="password" type="string"></property>
  <property name="repassword" column="repassword" type="string"></property>
  
  </class>
  </hibernate-mapping>